@model Cde.Models.FileCommand

<form asp-page-handler="File" enctype="multipart/form-data" method="post">
    <div class="form-group">
        <label asp-for="CommentText" class="col-md-2 control-label"></label>
        <div class="col-md-10">
            <input asp-for="CommentText" class="form-control"/>
            <span asp-validation-for="CommentText" class="text-danger"></span>
        </div>
    </div>
    <div class="form-group">
        <label asp-for="FileName" class="col-md-2 control-label"></label>
        <div class="col-md-10">
            <input asp-for="FileName" class="form-control"/>
            <span asp-validation-for="FileName" class="text-danger"></span>
        </div>
    </div>
    <div class="form-group">
        <label asp-for="IsText" class="col-md-2 control-label"></label>
        <div class="col-md-10">
            <input asp-for="IsText" class="form-control"/>
            <span asp-validation-for="IsText" class="text-danger"></span>
        </div>
    </div>
    <div class="form-group">
        <label asp-for="DocumentFile" class="col-md-2 control-label"></label>
        <div class="col-md-10">
            <input asp-for="DocumentFile" class="form-control"/>
            <span asp-validation-for="DocumentFile" class="text-danger"></span>
        </div>
    </div>
    <button type="submit" class="btn btn-primary">File</button>
</form>